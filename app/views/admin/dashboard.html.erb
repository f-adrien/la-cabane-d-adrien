  <div class='page-title-section'>
    <h1 class='page-title'>Dashboard</h1>
  </div>
  <div class="mb-4">
    <div class='a__box'>
      <div class="d-flex filter-form admin-inline-form">
        <div class="mr-4">
          <label for="set_period_start_date">DATE DE DÃ‰BUT</label>
          <input class="form-control datepicker" value="<%= @start_date.strftime("%d/%m/%Y") %>" data-datepicker-target="startDate" data-action="focusout->datepicker#correctDate" id="set_period_start_date">
        </div>
        <div class="mr-4">
          <label for="set_period_end_date">DATE DE FIN</label>
